<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.strind.wemedia.mapper.WmNewsMapper">

    <select id="getArticleId" resultType="java.lang.Long">
        select article_id from wm_news
        where id = #{id}
    </select>
</mapper>